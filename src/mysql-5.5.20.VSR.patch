+      {  
+		xid_count++;
+		cache_data->cookie = current_binlog_id;
+	  }
+    }
+	if(write_count > 0)
+	{
+	  bool synced = 0;
+	  if(flush_and_sync(&synced))
+	  {
+        for(current = queue; current != NULL; current = current->next)
+		{
+		  if(!current->error)
+		  {
+		    current->error = ER_ERROR_ON_WRITE;
+			current->commit_errno = errno;
+		  }
+		}
+	  }
