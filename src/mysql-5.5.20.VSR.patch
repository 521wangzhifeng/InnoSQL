+	  current = next;
+	}
+	DEBUG_SYNC(leader->thd, "commit_after_group_run_commit_ordered");
+	mysql_mutex_unlock(&LOCK_commit_ordered);
+  }
+
+  if(xid_count > 0 && check_purge)
+  {
+	purge();
+  }
+  DBUG_VOID_RETURN;
+}
